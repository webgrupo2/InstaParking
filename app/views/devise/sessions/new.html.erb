          <div class="r1 row">
            <div class="c1 col-md-12">
              <!-- Map accordion -->
              <div class="panel-group" id="contact-accordion-1">
                <div class="panel panel-default">
                  <h3 class="map-title">
                      <a data-toggle="collapse" data-parent="#contact-accordion-1" href="#contact-collapse-1" aria-expanded="true" aria-controls="contact-collapse-1"><span class="glyphicon glyphicon-map-marker"></span><br>Ubicanos</a>
                    </h3>
                  <div class="panel-collapse collapse in" id="contact-collapse-1">
                    <div class="panel-body">
                      <!-- Map -->
                      <div class="map-wrapper">
                        <!-- Here is where the map will be rendered -->
                        <div id="map-canvas"></div>
                        <!-- Address over the map -->
                        <address class="alert vcard">
                          <button class="close" data-dismiss="alert" type="button"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span>
                          </button>
                          <div class="org">
                            <strong>CarParkExpress</strong>
                          </div>
                          <div class="adr">
                            <span class="street-address">UPC</span>
                            <span class="locality">MONTERRICO</span>,
                            <span class="region">666</span> -
                            <span class="country-name">PERU</span>
                          </div>
                          <div class="tel">
                            <span class="tel-1">+1987 654 321</span>
                            <span class="tel-2">+987 654 321</span>
                          </div>
                          <div class="email">
                            <!-- Anti spam protection. Keep the codes: "&#x3a;" (= ":") / "&#x40;" (= "@") / "&#x2e;" (= ".") -->
                            <span class="email-contact"><a href="mailto&#x3a;EPE1&#x40;UPC&#x2e;com" data-user="EPE" data-domain="UPC.EDU.PE"></a></span>
                            <span class="email-support"><a href="mailto&#x3a;EPE2&#x40;UPC&#x2e;com" data-user="EPE2" data-domain="UPC.EDU.PE"></a></span>
                          </div>
                        </address>
                        <!-- End Address over the map -->
                      </div>
                      <!-- End Map -->
                    </div>
                  </div>
                </div>
              </div>
              <!-- End Map accordion -->
            </div>
          </div>
