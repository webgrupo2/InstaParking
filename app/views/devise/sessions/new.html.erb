              <div class="team-wrapper">
                <!-- Photo -->
                <div class="team-photo">
                  <%= image_tag spot.photo.url(:small) %>
                </div>
                <div class="team-content">
                  <h3 class="name"><%= spot.district.name %><br><small class="function">Precio: <%= spot.cost %></small></h3>
